<include>
  <user id="mm">
    <params>
      <param name="password" value="mm"/>
      <param name="vm-password" value="mm"/>
    </params>
    <variables>
      <variable name="toll_allow" value="domestic,international,local"/>
      <variable name="accountcode" value="mm"/>
      <variable name="user_context" value="default"/>
      <variable name="effective_caller_id_name" value="Extension mm"/>
      <variable name="effective_caller_id_number" value="mm"/>
      <variable name="outbound_caller_id_name" value="$${mm}"/>
      <variable name="outbound_caller_id_number" value="$${mm}"/>
      <variable name="callgroup" value="techsupport"/>
    </variables>
  </user>
</include>
