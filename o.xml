<include>
  <user id="o">
    <params>
      <param name="password" value="o"/>
      <param name="vm-password" value="o"/>
    </params>
    <variables>
      <variable name="toll_allow" value="domestic,international,local"/>
      <variable name="accountcode" value="o"/>
      <variable name="user_context" value="default"/>
      <variable name="effective_caller_id_name" value="Extension o"/>
      <variable name="effective_caller_id_number" value="o"/>
      <variable name="outbound_caller_id_name" value="$${o}"/>
      <variable name="outbound_caller_id_number" value="$${o}"/>
      <variable name="callgroup" value="techsupport"/>
    </variables>
  </user>
</include>
